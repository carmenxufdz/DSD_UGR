/*
CALCULADORA VERSION 1.0 BASICA
4 OPERACIONES
    + Suma
    - Resta
    x Multiplicacion
    / Division
*/

program CALCULADORA {
	version BASICA {
		double SUMA(double,double) = 1;
		double RESTA(double,double) = 2;
		double MULTIPLICA(double,double) = 3;
		double DIVIDE(double,double) = 4;
	}=1;
} = 0x20000155;